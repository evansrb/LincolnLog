  http://go.microsoft.com/fwlink/?LinkId=169433
  -->

<configuration>
    <connectionStrings>
              <add name="lincolnlog_productionConnectionString" connectionString="Data Source=.\SQLEXPRESS;Initial Catalog=lincolnlog;Integrated Security=True" providerName="System.Data.SqlClient" />
    </connectionStrings>
  <appSettings>
    <add key="RemoveWhitespace" value="true" />
    <add key="ConvertViewLinks" value="true" />
  </appSettings>
    <system.web>
        <compilation debug="true" targetFramework="4.0" />

    </system.web>
    <system.webServer>
        <defaultDocument>
            <files>
                <add value="home.aspx" />
            </files>
        </defaultDocument>
    </system.webServer>
	<system.web>
		<customErrors mode="Off"/>
	</system.web>

</configuration>
